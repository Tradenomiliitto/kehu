    "debug": "4.1.0",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-flash": "0.0.2",
    "express-https-redirect": "1.0.0",
    "express-session": "1.15.6",
    "express-validator": "5.3.0",
    "http-errors": "1.7.1",
    "jquery": "3.3.1",
    "knex": "0.15.2",
    "method-override": "3.0.0",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "objection": "1.3.0",
    "open-iconic": "1.1.1",
    "passport": "0.4.0",
