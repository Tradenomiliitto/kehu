    "node-sass": "4.9.3",
    "prettier": "1.14.3",
    "pretty-quick": "1.7.0",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
