{
  "name": "kehu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:prod && npm run migrate:latest",
    "build:watch": "npx webpack --env dev --watch",
    "build:prod": "NODE_ENV=production npx webpack --env prod",
    "lint": "eslint --ext js . ; stylelint '**/*.{css,scss}'",
    "lint:fix": "eslint --fix --ext js . ; stylelint --fix '**/*.{css,scss}'",
    "migrate:latest": "npx knex migrate:latest",
    "start": "node ./bin/www",
    "start:dev": "npx webpack --env dev && nodemon ./bin/www",
    "start:dev+build": "WEBPACK_HOT_RELOAD=true nodemon ./bin/www",
    "start:prod": "NODE_ENV=production node ./bin/www",
    "start:debug": "npx webpack --env dev && nodemon --inspect ./bin/www",
    "test": "npx nightwatch && npx jest",
    "jest:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@contentful/rich-text-html-renderer": "14.1.2",
