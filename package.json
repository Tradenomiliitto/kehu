  },
  "engines": {
    "node": "^20",
    "npm": ">=10"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "public/*",
      "tests/*"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged || true"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "stack": "heroku-20",
  "heroku-run-build-script": true
}
