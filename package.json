    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.2",
    "i18next-http-middleware": "3.5.0",
    "jspdf": "2.5.1",
    "knex": "3.1.0",
    "method-override": "3.0.0",
    "mini-css-extract-plugin": "2.7.6",
